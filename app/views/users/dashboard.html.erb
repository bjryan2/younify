<article class="user-dash">
  <section class="row search-bar card">
    <div class="tag-container">
      <input type="text" id="tokenfield" name="tags" placeholder="" class="tm-input"/>
    </div>
  </section>

  <section class="row card search-results">
    <div class="connections-wrapper col-md-12">

      <div class="connection  row connection">
        <div class="col-md-2 col-md-offset-1">
          <%= image_tag "person.jpg", :class => "profile-image profile-image-small" %>
        </div>
        <div class="col-md-6 col-md-offset-1">
          <h4 class="name">Jenny Murphy</h4>
          <p class="title"><b>Software Engineer</b> - Google</p>
        </div>
        <div class="col-md-1 col-md-offset-1">
          <i class="fa fa-envelope-o fa-2x"></i>
        </div>
      </div>

      <div class="connection  row connection">
        <div class="col-md-2 col-md-offset-1">
          <%= image_tag "person.jpg", :class => "profile-image profile-image-small" %>
        </div>
        <div class="col-md-6 col-md-offset-1">
          <h4 class="name">Jenny Murphy</h4>
          <p class="title"><b>Software Engineer</b> - Google</p>
        </div>
        <div class="col-md-1 col-md-offset-1">
          <i class="fa fa-envelope-o fa-2x"></i>
        </div>
      </div>

      <div class="connection  row connection">
        <div class="col-md-2 col-md-offset-1">
          <%= image_tag "person.jpg", :class => "profile-image profile-image-small" %>
        </div>
        <div class="col-md-6 col-md-offset-1">
          <h4 class="name">Jenny Murphy</h4>
          <p class="title"><b>Software Engineer</b> - Google</p>
        </div>
        <div class="col-md-1 col-md-offset-1">
          <i class="fa fa-envelope-o fa-2x"></i>
        </div>
      </div>
    </div>

    <!-- Modal -->
    <div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
      <div class="modal-dialog">
        <div class="modal-content">
          <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title" id="myModalLabel">Send an email to Jenny Murphy</h4>
          </div>
          <div class="modal-body">
          </div>
          <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            <button type="button" class="btn btn-primary">Send mail</button>
          </div>
        </div>
      </div>
    </div>

  </section>

</article>