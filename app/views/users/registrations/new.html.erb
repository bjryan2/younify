<div class="container">

  <div class="content">

    <article class="card signin-dash">


      <header>
        <h1>Sign up</h1>
      </header>


      <section class="errors">
        <%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
        <%= devise_error_messages! %>
        <% end %>
      </section>

      <section class="card oauth-cards links">
        <%= render "users/shared/links" %>

      </section>


    </article>
  </div>
</div>
